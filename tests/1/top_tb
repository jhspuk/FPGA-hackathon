#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5af8a767d4c0 .scope module, "top_tb" "top_tb" 2 3;
 .timescale -9 -12;
v0x5af8a76ca370_0 .var "clk", 0 0;
v0x5af8a76ca440_0 .net "q", 3 0, v0x5af8a76ca170_0;  1 drivers
v0x5af8a76ca510_0 .var "rst", 0 0;
S_0x5af8a767d650 .scope module, "uut" "top" 2 11, 3 1 0, S_0x5af8a767d4c0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "q";
v0x5af8a767d850_0 .net "clk", 0 0, v0x5af8a76ca370_0;  1 drivers
v0x5af8a76ca170_0 .var "q", 3 0;
v0x5af8a76ca250_0 .net "rst", 0 0, v0x5af8a76ca510_0;  1 drivers
E_0x5af8a76b8ac0 .event posedge, v0x5af8a76ca250_0, v0x5af8a767d850_0;
    .scope S_0x5af8a767d650;
T_0 ;
    %wait E_0x5af8a76b8ac0;
    %load/vec4 v0x5af8a76ca250_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5af8a76ca170_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5af8a76ca170_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5af8a76ca170_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5af8a767d4c0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5af8a76ca370_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5af8a76ca370_0;
    %inv;
    %store/vec4 v0x5af8a76ca370_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5af8a767d4c0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5af8a76ca510_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5af8a76ca510_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5af8a76ca510_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 36 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5af8a767d4c0;
T_3 ;
    %vpi_call 2 41 "$monitor", "At time %t, q = %h", $time, v0x5af8a76ca440_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5af8a767d4c0;
T_4 ;
    %vpi_call 2 46 "$dumpfile", "top_tb.vcd" {0 0 0};
    %vpi_call 2 47 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5af8a767d4c0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "top_tb.v";
    "top.v";
